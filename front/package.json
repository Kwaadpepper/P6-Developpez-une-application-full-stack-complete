{
  "name": "front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve --configuration development",
    "prod": "ng serve --configuration production",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "npm-run-all -s -c lint:ts lint:css",
    "lint:fix": "npm-run-all -s -c lint:ts:fix lint:css:fix",
    "lint:ts": "ng lint",
    "lint:ts:ci": "ng lint --format json --silent --quiet",
    "lint:ts:fix": "ng lint --fix",
    "lint:css": "node ./node_modules/.bin/stylelint --config './.stylelintrc.json' --max-warnings 0 \"src/**/*.css\"",
    "lint:css:ci": "node ./node_modules/.bin/stylelint --config './.stylelintrc.json' --max-warnings 0 \"src/**/*.css\" -f json",
    "lint:css:fix": "node ./node_modules/.bin/stylelint --config './.stylelintrc.json' --max-warnings 0 \"src/**/*.css\" --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@primeng/themes": "^19.0.5",
    "@tailwindcss/typography": "^0.5.16",
    "lodash-es": "^4.17.21",
    "ngx-markdown": "^19.0.0",
    "primeicons": "^7.0.0",
    "primeng": "^19.0.5",
    "rxjs": "~7.8.0",
    "tailwind-bootstrap-grid": "^5.1.0",
    "tailwindcss-primeui": "^0.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.7",
    "@angular/cli": "^19.0.7",
    "@angular/compiler-cli": "^19.0.0",
    "@stylistic/eslint-plugin": "^2.13.0",
    "@types/jasmine": "~5.1.0",
    "@types/lodash-es": "^4.17.12",
    "angular-eslint": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "csstype": "^3.1.3",
    "daisyui": "^5.0.0-beta.1",
    "eslint": "^9.18.0",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.1",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.1",
    "stylelint": "^16.13.0",
    "stylelint-actions-formatters": "^16.3.1",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-standard": "^37.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.20.0"
  }
}
